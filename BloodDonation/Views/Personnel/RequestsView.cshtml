@model BloodDonation.Models.RequestList

@{
    ViewBag.Title = "RequestsView";
}



<html lang="en" dir="ltr">
<head>
    <title>Requests</title>
    <meta charset="iso-8859-1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Styles.Render("~/Styles/layout_personel_add_blood.css")
    @Styles.Render("~/Styles/mediaqueries.css")
    <script src="@Url.Content(" ~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>

    <script src="@Url.Content(" ~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
</head>
<body>

    <div class="wrapper row1">
        <header id="header" class="clear">
            <div id="hgroup">
                <h1><a href="#">View all requests</a></h1>
                <h2><a href="#">Donation center personnel</a></h2>
            </div>
            @Html.Partial("NavigationBannerView", null)
        </header>
    </div>

    <div class="wrapper row1">
        
            <header id="header" class="clear">
                <nav>
                    <ul>
                        <li>@Html.ActionLink("Pending Requests", "PendingRequests", "Personnel", null, new { @class = "header nav li" })</li>
                        <li>@Html.ActionLink("Accepted Requests", "AcceptedRequests", "Personnel", null, new { @class = "header nav li" })</li>
                    </ul>
                </nav>
            </header>   
    </div>
    <!-- Footer -->
    <div class="wrapper row4">
        <footer id="footer" class="clear">
            <p class="fl_left">Copyright &copy; 2013 - All Rights Reserved - <a href="#">Domain Name</a></p>
        </footer>
    </div>
</body>
</html>
